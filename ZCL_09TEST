CLASS zcl_09test DEFINITION
  PUBLIC
  FINAL
  CREATE PUBLIC .

  PUBLIC SECTION.

    INTERFACES if_oo_adt_classrun .
  PROTECTED SECTION.
  PRIVATE SECTION.
ENDCLASS.



CLASS zcl_09test IMPLEMENTATION.


  METHOD if_oo_adt_classrun~main.
    DATA(first) = NEW zcl_09first( 1 ).
    DATA(second) = NEW zcl_09second( 2 ).
    out->write( |itemvar variable: { first->gettype( ) }| ).
    out->write( |itemvar variable: { second->gettype( ) }| ).
    first->runpolytest( out ).
    second->runpolytest( out ).
    first->publicpolytest( out ).
    second->publicpolytest( out ).
    out->write( 'ready' ).
  ENDMETHOD.
ENDCLASS.
